// To go in the component.json file for each page
    {
        "_id": "c-04",
        "_parentId": "b-04",
        "_type": "component",
        "_component": "quicknav",
        "_classes": "",
        "_layout": "left",
        "title": "",
        "displayTitle": "",
        "body": "",
        "instruction": "",
        "_quicknav": {
            "_comment": "allPages = loop sequentially through all pages in course",
            "_comment": "siblings = loop sequentially through all pages in current parent object",
            "_comment": "none = disable previous and next buttons at start and end of the pages in the current parent object",
            "_loopType": "false",
            "_buttons": {
                "_previous": {
                    "_isEnabled": true,
                    "_order": 1,
                    "_classes": "",
                    "_icon": "",
                    "text": "< Previous",
                    "ariaLabel": "Previous Page",
                    "_showTooltip": true,
                    "tooltip": "{{displayTitle}}",
                    "_customRouteId": ""
                },
                "_root": {
                    "_isEnabled": true,
                    "_order": 1,
                    "_classes": "",
                    "_icon": "",
                    "text": "Go to main menu",
                    "ariaLabel": "Got to Main menu",
                    "_showTooltip": true,
                    "tooltip": "{{displayTitle}}",
                    "_customRouteId": ""
                },
                "_up": {
                    "_isEnabled": true,
                    "_order": 1,
                    "_classes": "",
                    "_icon": "",
                    "text": "Back to menu",
                    "ariaLabel": "Back to menu",
                    "_showTooltip": true,
                    "tooltip": "{{displayTitle}}",
                    "_customRouteId": "co-10"
                },
                "_next": {
                    "_isEnabled": true,
                    "_order": 1,
                    "_classes": "",
                    "_icon": "",
                    "text": "Next >",
                    "ariaLabel": "Next Page",
                    "_showTooltip": true,
                    "tooltip": "{{displayTitle}}",
                    "_customRouteId": ""
                },
                "_sibling": {
                    "_isEnabled": true,
                    "_order": 1,
                    "_classes": "",
                    "_icon": "",
                    "text": "{{inc index}}",
                    "ariaLabel": "Page {{inc index}}",
                    "_showTooltip": true,
                    "tooltip": "{{displayTitle}}",
                    "_customRouteId": ""
                }
            }
        }
    },